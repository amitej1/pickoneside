<!-- <<<<<<< HEAD -->
<%= render partial: 'layouts/header' %> 
<div class="offset3 span9" >
  <div class='span6'>
    <div class="row-fluid">
      <div class="box-it">
        <div align="center">
          <div class="heading" style="display:inline !important;"> 
            Sign Up
          </div>
        </div>
      </div><br/>
      <div class="row-fluid">
        <div class="box-it">

            <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
            
              <form class="form-horizontal">
                <div class="control-group">
                  <div class="control-label"><%= f.label :username %> </div>
                  <div class='controls'>
                    <%= f.text_field :username %>
                  </div>
                </div>
                <div class="control-group">
                  <div class="control-label"><%= f.label :email %> </div>
                  <div class='controls'>
                    <%= f.email_field :email %>
                  </div>
                </div>
                <div class="control-group">
                  <div class="control-label"><%= f.label :password %> </div>
                  <div class='controls'>
                    <%= f.password_field :password  %>
                  </div>
                </div>
                <div class="control-group">
                  <div class="control-label"><%= f.label :password_confirmation %> </div>
                  <div class='controls'>
                    <%= f.password_field :password_confirmation   %>
                  </div>
                </div>
                <div class="control-group">
                  <div class="control-label"><%= f.label ' ' %> </div>
                  <div class='controls'>
                    <%= f.submit 'Sign Up', :class=>'btn btn-inverse' %>
                  </div>
                </div>
              <div class="control-group" align="center">
              <div class="control-label"><%= f.label ' ' %> </div>
                <div class='controls'>
                  <%= render "links" %>
                </div>
              </div>
          </form>
        <% end %>
 
    </div>
  </div>
</div>

 


</div>
</div>
<%= render partial: 'layouts/footer' %> 


